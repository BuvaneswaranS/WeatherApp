<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/current_weather_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0">

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_clouds_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_city_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_clouds_text_data"
            app:layout_constraintBottom_toTopOf="@id/current_dt_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside" />

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_clouds_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/current_clouds_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/current_dt_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_dt_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_date_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_clouds_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_dt_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_feels_like_text"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_dt_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolkata"
            app:layout_constraintTop_toBottomOf="@id/current_clouds_text_data"
            app:layout_constraintStart_toEndOf="@id/current_dt_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_feels_like_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_feels_like_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_feels_like_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_dt_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_feels_like_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_pressure_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_feels_like_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_dt_text_data"
            app:layout_constraintStart_toEndOf="@id/current_feels_like_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_pressure_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_pressure_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_pressure_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_feels_like_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_pressure_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_humidity_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_pressure_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_feels_like_text_data"
            app:layout_constraintStart_toEndOf="@id/current_pressure_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_humidity_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_humidity_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_humidity_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_pressure_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_humidity_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_temp_text"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_humidity_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_pressure_text_data"
            app:layout_constraintStart_toEndOf="@id/current_humidity_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_temp_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_temp_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_temp_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_humidity_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_temp_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_sunrise_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_temp_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_humidity_text_data"
            app:layout_constraintStart_toEndOf="@id/current_temp_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_sunrise_text_data"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_sunrise_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_sunrise_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_temp_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_sunrise_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_sunset_text"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_sunrise_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_temp_text_data"
            app:layout_constraintStart_toEndOf="@id/current_sunrise_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_sunset_text_data"/>


        <TextView
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:id="@+id/current_sunset_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_sunset_text"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_sunrise_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_sunset_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_visibility_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:id="@+id/current_sunset_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolkata"
            app:layout_constraintTop_toBottomOf="@id/current_sunrise_text_data"
            app:layout_constraintStart_toEndOf="@id/current_sunset_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_visibility_text_data"/>

        <TextView
            android:id="@+id/current_visibility_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_visibility_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_sunset_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_visibility_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_wind_deg_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            android:id="@+id/current_visibility_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_sunset_text_data"
            app:layout_constraintStart_toEndOf="@id/current_visibility_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_wind_deg_text_data"/>

        <TextView
            android:id="@+id/current_wind_deg_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_wind_deg_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_visibility_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_wind_deg_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_wind_speed_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            android:id="@+id/current_wind_deg_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_visibility_text_data"
            app:layout_constraintStart_toEndOf="@id/current_wind_deg_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_wind_speed_text_data"/>


        <TextView
            android:id="@+id/current_wind_speed_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_wind_speed_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_wind_deg_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_wind_speed_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_city_text"/>

        <TextView
            android:id="@+id/current_wind_speed_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_wind_deg_text_data"
            app:layout_constraintStart_toEndOf="@id/current_wind_speed_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_city_text_data"/>

        <TextView
            android:id="@+id/current_city_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_cloud_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_wind_speed_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_city_text_data"
            app:layout_constraintBottom_toTopOf="@+id/current_main_text"
            app:layout_constraintHorizontal_chainStyle="spread_inside"/>

        <TextView
            android:id="@+id/current_city_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_wind_speed_text_data"
            app:layout_constraintStart_toEndOf="@id/current_city_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_main_text_data"/>

        <TextView
            android:id="@+id/current_main_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_main_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_city_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_main_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_description_text"/>

        <TextView
            android:id="@+id/current_main_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_city_text_data"
            app:layout_constraintStart_toEndOf="@id/current_main_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_description_text_data" />

        <TextView
            android:id="@+id/current_description_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_description_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_main_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_description_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toTopOf="@+id/current_icon_text"/>

        <TextView
            android:id="@+id/current_description_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_main_text_data"
            app:layout_constraintStart_toEndOf="@id/current_description_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/current_icon_text_data"/>

        <TextView
            android:id="@+id/current_icon_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weatherApp_main_activity_current_weather_icon_text"
            style="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginStart="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginBottom="@dimen/guidelines_margin_padding_16dp"
            app:layout_constraintTop_toBottomOf="@id/current_description_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/current_icon_text_data"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/current_icon_text_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.MaterialComponents.Body1"
            android:layout_marginTop="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginEnd="@dimen/guidelines_margin_padding_16dp"
            android:layout_marginBottom="@dimen/guidelines_margin_padding_16dp"
            tools:text="Kolata"
            app:layout_constraintTop_toBottomOf="@id/current_description_text_data"
            app:layout_constraintStart_toEndOf="@id/current_icon_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>